{
  "name": "personal-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "npm run unflow && npm run remove-imports && mocha",
    "flow": "flow",
    "unflow": "flow-remove-types src/helpers --out-dir flow/",
    "remove-imports": "bash src/scripts/remove-imports.sh",
    "clean": "npm cache verify",
    "prettier": "prettier --write src/**/*.vue src/**/*.js test/**/*.js",
    "prettier:check": "prettier --check src/**/*.vue src/**/*.js test/**/*.js",
    "now-dev": "vue-cli-service serve  --port=$PORT",
    "now-build": "vue-cli-service build",
    "deploy": "now",
    "deploy:debug": "now -d"
  },
  "dependencies": {
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.9",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.9",
    "@vue/cli-service": "^3.1.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-flowtype-errors": "^3.5.1",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-vue": "^4.5.0",
    "flow-bin": "^0.72.0",
    "flow-remove-types": "^1.2.3",
    "mocha": "^5.2.0",
    "node-sass": "^4.10.0",
    "prettier": "1.17.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.13"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
